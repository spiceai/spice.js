    "test": "jest",
    "dev": "npx nodemon --exec ts-node ./test/index.ts --watch test --ext .ts",
    "preversion": "yarn build"
  },
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "@types/node": "^20.14.2",
    "@types/retry": "^0.12.5",
    "@types/ws": "^8.5.4",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.3.1",
    "jest": "^29.5.0",
    "nodemon": "^3.0.2",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "ws": "^8.13.0"
  }
}
