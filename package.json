{
  "name": "@spiceai/spice",
  "description": "JS + TS SDK for spice.ai",
  "version": "2.0.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": "git@github.com:spiceai/spice.js.git",
  "author": "Spice AI, Inc. <webmaster@spice.ai>",
  "license": "MIT",
  "dependencies": {
    "@grpc/grpc-js": "1.10.6",
    "@grpc/proto-loader": "0.7.12",
    "apache-arrow": "16.0.0",
    "node-fetch": "^2.6.9",
    "retry": "^0.13.1"
  },
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "yarn clean",
    "build": "tsc",
    "postbuild": "copyfiles proto/* dist",
